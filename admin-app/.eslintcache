ult Home;\r\n","E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\container\\singin\\index.js",["129","130","131"],"import React, { useEffect, useState } from \"react\";\r\nimport { Container, Form, Button, Row, Col } from \"react-bootstrap\";\r\nimport Layout from \"../../components/layouts\";\r\nimport Input from \"../../components/ui\";\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport { login } from '../../actions'\r\nimport { Redirect } from 'react-router-dom'\r\n\r\nconst Signin = (props) => {\r\n\r\n  const [email, setEmail] = useState('');\r\n  const [password, setPassword] = useState('');\r\n  const [error, setError] = useState('');\r\n  const auth = useSelector(state => state.auth)\r\n\r\n  const dispatch = useDispatch();\r\n\r\n\r\n\r\n  // login function\r\n  const userLogin = e => {\r\n    e.preventDefault();\r\n\r\n    const user = { email, password }\r\n\r\n    dispatch(login(user));\r\n  }\r\n\r\n  if (auth.authenticate) {\r\n    return <Redirect to={`/`} />\r\n  }\r\n\r\n  return (\r\n    <Layout>\r\n      <Container>\r\n        <Row style={{ marginTop: \"50px\" }}>\r\n          <Col md={{ span: 6, offset: 3 }}>\r\n            <Form onSubmit={ userLogin }>\r\n              <Input\r\n                label=\"Email\"\r\n                placeholder=\"Enter email\"\r\n                value={email}\r\n                type=\"email\"\r\n                onChange={(e) => setEmail(e.target.value)}\r\n              />\r\n\r\n              <Input\r\n                label=\"Password\"\r\n                placeholder=\"Enter password\"\r\n                value={password}\r\n                type=\"password\"\r\n                onChange={(e) => setPassword(e.target.value )}\r\n              />\r\n              <Button variant=\"primary\" type=\"submit\">\r\n                Submit\r\n              </Button>\r\n            </Form>\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </Layout>\r\n  );\r\n};\r\n\r\nexport default Signin;\r\n","E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\container\\signup\\index.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\components\\ui\\index.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\store\\index.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\reducers\\index.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\actions\\index.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\actions\\auth.js",["132"],"import { authConstants, signupConstants } from \"./constant\"\r\nimport axios from '../helpers/axios'\r\n\r\n// login function\r\nexport const login = (user) => {\r\n    return async (dispatch) => {\r\n        dispatch({ type: authConstants.LOGIN_REQUEST });\r\n\r\n        const res = await axios.post(`/admin/signin`, {\r\n            ...user\r\n        })\r\n\r\n        if (res.status === 200) {\r\n            const { token, user } = res.data;\r\n            localStorage.setItem('token', token);\r\n            localStorage.setItem('user', JSON.stringify(user));\r\n            dispatch({\r\n                type: authConstants.LOGIN_SUCCESS,\r\n                payload: { token, user }\r\n            });\r\n        }else if (res.status === 400) {\r\n            dispatch({\r\n                type: authConstants.LOGIN_FAILURE,\r\n                payload: { error: res.data.error }\r\n            });\r\n        }\r\n    }\r\n}\r\n\r\n// check user has logged \r\nexport const isUserLoggedIn = () => {\r\n    return async dispatch => {\r\n        const token = localStorage.getItem('token');\r\n        if (token) {\r\n            const user = JSON.parse(localStorage.getItem('user'));\r\n            dispatch({\r\n                type: authConstants.LOGIN_SUCCESS,\r\n                payload: { token, user }\r\n            });\r\n        }else {\r\n            dispatch({\r\n                type: authConstants.LOGIN_FAILURE,\r\n                payload: { error: 'Failed to login' }\r\n            });\r\n        }\r\n    }\r\n}\r\n\r\n// signout user action\r\nexport const signout = () => {\r\n    return async dispatch => {\r\n        dispatch({ type: authConstants.LOGOUT_REQUEST })\r\n\r\n        const res = await axios.post(`/admin/signout`);\r\n\r\n        if (res.status === 200) {\r\n            localStorage.clear();\r\n            dispatch({\r\n                type: authConstants.LOGOUT_SUCCESS\r\n            });   \r\n        }else {\r\n            dispatch({\r\n                type: authConstants.LOGOUT_FAILURE,\r\n                payload: {\r\n                    error: res.data.error\r\n                }\r\n            })\r\n        }\r\n    }\r\n}","E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\actions\\constant.js",[],"E:\\Tutorial\\projects\\e-commerce\\admin-app\\src\\reducers\\auth.js",["133","134"],"import { authConstants } from \"../actions/constant\";\r\n\r\nconst initState = {\r\n    token: null,\r\n    user: {\r\n        firstName: '',\r\n        lastName: '',\r\n        email: ''\r\n    },\r\n    authenticate: false,\r\n    authenticating: false,\r\n    loading: false,\r\n    error: null,\r\n    message: ''\r\n};\r\n\r\n\r\n// loging reducer\r\nexport default (state = initState, action) => {\r\n    console.log(action)\r\n\r\n    switch(action.type) {\r\n        case authConstants.LOGIN_REQUEST:\r\n            state ={\r\n                ...state,\r\n                authenticating: true\r\n            }\r\n            break;\r\n        case authConstants.LOGIN_SUCCESS:\r\n            state = {\r\n                ...state,\r\n                user: action.payload.user,\r\n                token: action.payload.token,\r\n                authenticate: true,\r\n                authenticating: false\r\n            }\r\n            break;\r\n        case authConstants.LOGOUT_REQUEST:\r\n            state = {\r\n                loading: true\r\n            }\r\n            break;\r\n        case authConstants.LOGOUT_SUCCESS:\r\n            state = {\r\n      